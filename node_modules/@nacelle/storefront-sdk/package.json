{
  "version": "1.3.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "SDK_VERSION=${npm_package_version} dts watch",
    "build": "SDK_VERSION=${npm_package_version} dts build",
    "test": "SDK_VERSION=${npm_package_version} dts test --silent",
    "test:watch": "SDK_VERSION=${npm_package_version} dts test --verbose --watch",
    "test:coverage": "SDK_VERSION=${npm_package_version} dts test --coverage",
    "lint": "dts lint",
    "prepare": "SDK_VERSION=${npm_package_version} husky install && dts build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "gen:gql-types": "graphql-codegen --config codegen.yml"
  },
  "name": "@nacelle/storefront-sdk",
  "description": "Fetch data for your Nacelle-powered headless commerce project",
  "author": "Nacelle Inc.",
  "module": "dist/storefront-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/storefront-sdk.cjs.production.min.js",
      "limit": "70 KB"
    },
    {
      "path": "dist/storefront-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "@size-limit/preset-small-lib": "^6.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "dts-cli": "^0.19.4",
    "eslint": "^7.25.0",
    "graphql": "^15.6.1",
    "husky": "^7.0.4",
    "semantic-release": "^17.4.7",
    "size-limit": "^6.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@urql/core": "^2.4.3",
    "cross-fetch": "^3.1.4",
    "graphql-ws": "^5.6.4",
    "wonka": "^4.0.15"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  }
}
